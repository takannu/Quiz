<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.co.atschool.quiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/android_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/android_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuizActivity" />
        <activity android:name=".QuizResult" />
        <activity android:name=".SexSelectActivity" />
        <activity android:name=".SexSelectNextActivity"></activity>
    </application>

</manifest>